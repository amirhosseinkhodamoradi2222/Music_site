<%- include('./header/headeradmin.ejs')  %>  

    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
        <div class="row">
           
            <h3 class="mt-3 mb-3 ml-1">  ویرایش کردن اطلاعات کنسرت  </h3>
            <div class="col-12 col-lg-8">
                <form action="/admin/edtconsert<%- id %> " method="POST" enctype="multipart/form-data">
                <div class="mb-3 mt-3">
                    <label for="exampleFormControlInput1" class="">عنوان</label>
                    <input type="text" class="form-control" name="title" value="<%- titles %> " id="exampleFormControlInput1">
                  </div>
                  <div class="mb-3">
                    <label for="formFileMultiple" class="">عکس برای کنسرت</label>
                    <input class="form-control" name="img" value="<%- img %> " type="file" id="formFileMultiple" multiple>
                  </div>
                  <div class="mb-3 mt-3">
                    <label for="exampleFormControlInput1" class="">متن کنسرت</label>
                    <textarea class="form-control" id="exampleFormControlTextarea1" name="text" rows="5"><%- text %> </textarea>
                  </div>
                  <div class="mb-3">
                    <label for="exampleFormControlTextarea1" class="">تاریخ کنسرت</label>
                    <input type="text" class="form-control" value="<%- history %>" name="history" id="exampleFormControlInput1">
                </div>
                  <div class="mb-3">
                    <label for="exampleFormControlTextarea1" class="">ساعت برگزاری کنسرت</label>
                    <input type="text" class="form-control" id="exampleFormControlInput1" value="<%- date %>" name="date">
                </div>
                  <div class="mb-3">
                    <label for="exampleFormControlTextarea1" class=""> لینک خرید بلیط کنسرت</label>
                    <input type="text" class="form-control" id="exampleFormControlInput1" value="<%- link %>" name="link">
                </div>
                <div class="mb-3 mb-8">
                    <button type="submit" class="btn btn-success w-25">ثبت</button>
                  <button type="reset" class="btn btn-danger w-25">بازنشانی</button>
                </div>
                </form> 
            </div>
            <div class="col-12 col-lg-2"></div>
            <div class="col-12 col-lg-2"></div>
        </div>
    </main>
    
    
<%- include('./footer/footeradmin.ejs')  %> 