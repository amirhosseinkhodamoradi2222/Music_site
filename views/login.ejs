  <body dir="rtl" class="text-center font">
    
<main class="form-signin">
  <form method="POST" action="/admin/login"> 
    <img class="mb-4" src="../img/svg-music.png" alt="" width="72" height="57">
    <h1 class="h3 mb-3 fw-normal">ورود به پنل مدیریت</h1>
    <% if(masexite){ %> 
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
      خروج از  پنل مدیریت
       <button type="button" class="close" data-dismiss="alert" aria-label="Close">
         <span aria-hidden="true">&times;</span>
       </button>
     </div> 
     <% } %>  
    <div class="form-floating mb-1">
        <input type="text" class="form-control" id="floatingInput" name="user_name" placeholder="text" required>
        <label for="floatingInput">نام کاربری</label>
      </div>
      <div class="form-floating">
        <input type="password" class="form-control" id="Password" name="password" placeholder="Password" required>
        <label for="floatingPassword" >رمز عبور</label>
      </div>
    <div class="checkbox mb-3">
      <label>
        <input type="checkbox" value="remember-me" class="mt-3"> مرا به خاطر بسپار
      </label>
    </div>
    <button class="w-100 btn btn-lg btn-primary" type="submit">ورود</button>
   <p class="mt-5 mb-3 text-muted">&copy; وندا پلاس</p>
  </form>

</main>
